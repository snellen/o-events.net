  <table>
      <tr>
        <th><%=t('.registration')%></th>
        <th><%=t('.teampool')%></th>
        <th><%=t('.amount')%> [<%= @event.currency.iso_code %>]   </th>
      </tr>

    <% @teams.each do |team| %>
        <tr>
        <td>  <%= team.get_display_name %>  </td>
        <td>  <%= team.team_pool.name %>  </td>
        <td>  <%= format_amount(team.start_fee) %>  </td>
      </tr>
    <% end %>
  </table>
