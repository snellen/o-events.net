<h1>Listing payment_log_entries</h1>

<table>
  <tr>
    <th>Severity</th>
    <th>Controller name</th>
    <th>Action</th>
    <th>Payment</th>
    <th>Ip address</th>
    <th>Message</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @payment_log_entries.each do |payment_log_entry| %>
  <tr>
    <td><%= payment_log_entry.severity %></td>
    <td><%= payment_log_entry.controller_name %></td>
    <td><%= payment_log_entry.action %></td>
    <td><%= payment_log_entry.payment_id %></td>
    <td><%= payment_log_entry.ip_address %></td>
    <td><%= payment_log_entry.message %></td>
    <td><%= link_to 'Show', payment_log_entry %></td>
    <td><%= link_to 'Edit', edit_payment_log_entry_path(payment_log_entry) %></td>
    <td><%= link_to 'Destroy', payment_log_entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Payment log entry', new_payment_log_entry_path %>
