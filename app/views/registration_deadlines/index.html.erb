<h1>Listing registration_deadlines</h1>

<table>
  <tr>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @registration_deadlines.each do |registration_deadline| %>
  <tr>
    <td><%= registration_deadline.date %></td>
    <td><%= link_to 'Show', registration_deadline %></td>
    <td><%= link_to 'Edit', edit_registration_deadline_path(registration_deadline) %></td>
    <td><%= link_to 'Destroy', registration_deadline, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Registration deadline', new_registration_deadline_path %>
