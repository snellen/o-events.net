<h1>Listing payments</h1>

<table>
  <tr>
    <th>Reference number</th>
    <th>Valuta</th>
    <th>Fees valuta</th>
    <th>Registration time</th>
    <th>Security data</th>
    <th>Method</th>
    <th>Fees</th>
    <th>Fees included</th>
    <th>Amount</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @payments.each do |payment| %>
  <tr>
    <td><%= payment.reference_number %></td>
    <td><%= payment.valuta %></td>
    <td><%= payment.fees_valuta %></td>
    <td><%= payment.registration_time %></td>
    <td><%= payment.security_data %></td>
    <td><%= payment.method %></td>
    <td><%= payment.fees %></td>
    <td><%= payment.fees_included %></td>
    <td><%= payment.amount %></td>
    <td><%= link_to 'Show', payment %></td>
    <td><%= link_to 'Edit', edit_payment_path(payment) %></td>
    <td><%= link_to 'Destroy', payment, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Payment', new_payment_path %>
